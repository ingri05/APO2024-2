<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demoinicial.Controller.HelloController">
   <VBox >
      <padding>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
      </padding>

      <Label fx:id="lName" text="Name"/>
      <TextField fx:id="txtName"></TextField>

      <Button fx:id="btnAdd" text="Add" onAction="#registerUser"/>
   </VBox>
   <VBox>
      <Label fx:id="welcomeText" text=""></Label>
   </VBox>
</VBox>
